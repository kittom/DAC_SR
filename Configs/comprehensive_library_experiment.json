{
    "experiment": {
        "name": "ComprehensiveLibraryExperiment",
        "description": "Comprehensive experiment using library algorithms on all benchmarks: LeadingOnes, OneMax, and PSA-CMA-ES",
        "suite": "SuiteA"
    },
    "data_generation": {
        "benchmarks": {
            "leadingones": {
                "enabled": true,
                "instance_sizes": [10, 20, 30, 40, 50],
                "data_type": "continuous"
            },
            "onemax": {
                "enabled": true,
                "instance_sizes": [10, 20, 30, 40, 50],
                "data_type": "continuous"
            },
            "psacmaes": {
                "enabled": true,
                "sub_benchmarks": ["sphere", "ellipsoid", "rastrigin"],
                "instance_sizes": [10, 20],
                "data_type": "continuous",
                "iterations": 1000,
                "compare": true,
                "individual_benchmarks": true,
                "all_benchmarks": true
            },
            "model": {
                "enabled": false,
                "instance_sizes": [10, 20],
                "data_type": "continuous"
            }
        },
        "noise": {
            "enabled": false,
            "level": "1e-12",
            "type": "gaussian"
        },
        "dropout": {
            "enabled": false,
            "rate": 0.0,
            "type": "random_rows"
        }
    },
    "evaluation": {
        "analysis_styles": {
            "control": {
                "enabled": false,
                "description": "Default SR algorithms with minimal configuration"
            },
            "library": {
                "enabled": true,
                "description": "SR algorithms with library configurations"
            },
            "rounding": {
                "enabled": false,
                "description": "SR algorithms with rounding support"
            }
        },
        "metrics": {
            "nmse": true,
            "r2_score": true,
            "mae": true,
            "mse": true
        },
        "stopping_criteria": {
            "max_iterations": 100,
            "min_iterations": 10,
            "convergence_threshold": 1e-12,
            "time_limit_seconds": 3600
        }
    },
    "output": {
        "base_directory": "Experiments",
        "suite_name": "SuiteA",
        "experiment_name": "ComprehensiveLibraryExperiment",
        "results_format": "csv",
        "save_intermediate_results": true,
        "create_summary_report": true
    },
    "execution": {
        "parallel": {
            "enabled": false,
            "max_workers": 4
        },
        "logging": {
            "level": "INFO",
            "save_logs": true,
            "log_file": "experiment.log"
        },
        "cleanup": {
            "remove_intermediate_files": false,
            "compress_results": false
        }
    }
} 